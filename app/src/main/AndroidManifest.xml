<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.okeypuantablosu"
    android:installLocation="auto">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_circle"
        android:supportsRtl="true"
        android:theme="@style/Theme.OkeyPuanTablosu">
        <activity
            android:name=".diceroller.DiceRoller"
            android:theme="@style/Theme.DiceRoller"
            android:exported="false" />
        <activity
            android:name=".anamenu.AnaMenuActivity"
            android:exported="false" />
        <activity
            android:name=".puantablosu.PuanTablosu2Kisi"
            android:exported="false" />
        <activity
            android:name=".puantablosu.PuanTablosu3Kisi"
            android:exported="false" />
        <activity
            android:name=".puantablosu.PuanTablosu4Kisi"
            android:exported="false" />
        <activity
            android:name=".takimislemleri.TakimIslemleri"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>